{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cross-Origin-Embedder-Policy", "value": "require-corp" },
        { "key": "Cross-Origin-Opener-Policy",  "value": "same-origin" }
      ]
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/(.*\\.(js|wasm|pck|html|css|png|jpg|jpeg|svg|ogg|mp3))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
